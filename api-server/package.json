{
  "name": "@rate-limiter/api-server",
  "version": "1.0.0",
  "description": "REST API server for rate limiting - Language agnostic rate limiter accessible via HTTP",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "docker:build": "docker build -t rate-limiter-api .",
    "docker:run": "docker run -p 8080:8080 rate-limiter-api"
  },
  "keywords": [
    "rate-limiter",
    "api",
    "rest",
    "http",
    "microservice",
    "multi-language"
  ],
  "author": "Rate Limiter Contributors",
  "license": "MIT",
  "dependencies": {
    "express": "^5.2.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.4.5",
    "ioredis": "^5.4.2"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^7.2.2",
    "nodemon": "^3.1.9"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
